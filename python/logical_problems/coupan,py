import random

def generate_distinct_coupons(n):
    coupons = set()
    count = 0

    while len(coupons) < n:
        new_coupon = random.randint(1, n)
        coupons.add(new_coupon)
        count += 1

    print(f"Total random numbers generated: {count}")
    return count


generate_distinct_coupons(5)
